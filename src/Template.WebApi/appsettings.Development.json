{
  "AllowedOrigins": "https://example.se|https://example.se",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationName": "Template.WebApi"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    }
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "Enabled": true,
        "Authority": "https://example.se",
        "ValidAudiences": [
          "foo"
        ]
      }
    }
  }
}